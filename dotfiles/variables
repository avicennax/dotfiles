# bash_variables
# vim: filetype=sh
# DESCRIPTION: user defined shell variables loaded into shell session
#   by .bashrc or .bash_profile

src="$HOME/src"
bv="$HOME/.variables"
bl="$HOME/.locals"
ba="$HOME/.aliases"
bf="$HOME/.functions"
bp="$HOME/.bash_profile"
zshrc="$HOME/.zshrc"
vimrc="$HOME/.vimrc"
nvi="$HOME/.config/nvim/init.lua"
gitconfig="$HOME/.gitconfig"